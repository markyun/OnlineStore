    width: 200px;
    height: 200px;
    margin: 40px auto 20px;
}
.panel-bundle {
    margin-top: 0;
    text-align: center;
}
.panel-order {
    border-width: 0;
}
.panel-order > .panel-heading {
    padding: 0;
}
.panel-order > .panel-footer {
    background-color: #ffffff;
    border-top-width: 0;
    text-align: right;
}
.step-tips {
    width: 54px;
    height: 54px;
    position: absolute;
    background-color: #999;
}
.step-tips .current-step, .step-tips .total-step {
    position: absolute;
    width: 31.32px;
    height: 31.32px;
    text-align: center;
    line-height: 31.32px;
    color: #fff;
    font-size: 1.3rem;
}
.step-tips .current-step {
    top: 0;
    left: 0;
}
.step-tips .total-step {
    right: 0;
    bottom: 0;
}
.step-tips .boundary {
    height: 1px;
    margin: 0;
    padding: 0;
    width: 54px;
    color: #EEE;
    background-color: #EEE;
    position: absolute;
    top: 27px;
    -webkit-transform: rotate(-45deg);
    -moz-transform: rotate(-45deg);
    transform: rotate(-45deg);
}
.scroll-steps {
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    width: 100%;
    height: 54px;
    padding-left: 54px;
    display: table;
    table-layout: fixed;
}
.scroll-step-item {
    display: table-cell;
    vertical-align: middle;
    border-width: 0;
    padding-left: 12px;
    text-align: left;
    text-indent: 10px;
    font-size: 1.0rem;
    color: #999;
    font-weight: 300;
    background-color: #DDD;
}
.scroll-step-item.active {
    color: #FFF;
    background-color: #FF6008;
}
.scroll-step-item:nth-child(1) {
    width: 17%;
}
.scroll-step-item:nth-child(2) {
    width: 30%;
}
.scroll-step-item:nth-child(3) {
    width: 28%;
}
.scroll-step-item:nth-child(4) {
    width: 21%;
}
.step-icon {
    float: left;
    width: 18px;
    height: 18px;
}
.scroll-content {
    background-color: #FFF;
}
.number-box {
    float: left;
    position: relative;
    width: 14.286%;
    /* 1/7 */

    height: 70px;
    line-height: 60px;
    text-align: center;
    color: #333;
}
.js-num .number-box.active {
    color: #FFF;
    background-color: #FF6008;
}
.js-num.group .number-box.active {
    color: #333;
}
.number-box.active .number-price {
    color: #FFF;
}
.number-price {
    display: block;
    position: absolute;
    bottom: 0;
    width: 100%;
    height: 40px;
    line-height: 40px;
    text-align: center;
    color: #FF6008;
    font-size: 1.2rem;
    font-weight: bold;
}
.brand-info.active, .color-info.active, .camera-info.active {
    color: #FFF;
    background-color: #FF6008;
}
.btn-random {
    border-radius: 6px;
}
.form-number {
    margin-top: 20px;
    margin-bottom: 20px;
    text-align: center;
}
.cabin-info {
    width: 100%;
    font-size: 1.7em;
    line-height: 2;
    color: #000;
    text-align: center;
}
.cabin-info .sub-title {
    font-size: 1.0rem;
    color: #999;
}
.cabin-select, .cabin-btn {
    height: 45px;
    line-height: 45px;
    text-align: center;
    font-size: 1.2em;
    color: #999;
    border: 1px solid #999;
