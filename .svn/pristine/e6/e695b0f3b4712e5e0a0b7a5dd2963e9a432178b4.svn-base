}
.step-footer .btn + .btn {
    margin-left: 20px;
}
.panel-default {
    border-color: #DDDDDD;
}
.panel-default > .panel-heading {
    display: table;
    width: 100.26%;
    background-color: #DDDDDD;
    color: #333;
    text-align: center;
    min-height: 50px;
    padding-top: 5px;
}
.panel-default > .panel-heading >a.head_a{
    display: table-cell;
    vertical-align: middle;
    line-height: 16px;
    outline-offset: -4px;
    color: #333;
    text-decoration: none;
}
.panel-default.active > .panel-heading >a.head_a{
    color: #fff;
    text-decoration: none;
}

.panel-default > .panel-heading >a.head_a:hover{
    text-decoration: none;
}
.panel-default > .panel-body {
    color: #666;
    padding: 20px;
}
.panel-default.active {
    border-color: #FF6008;
}
.panel-default.active .panel-heading {
    background-color: #FF6008;
    color: #ffffff;
    border-color: #FF6008;
}
.group-section {
    margin-top: 36px;
    width: 768px;
    margin: 0 auto;
}
.group-box {
    display: inline-block;
    width: 135px;
    height: 135px;
    border: 1px solid #DDD;
    border-radius: 50%;
    background-color: #FFF;
    overflow: hidden;
}
.group-box + .group-box {
    margin-left: 68px;
}
.group-box.active {
    border: 1px solid #FF6008;
}
.group-box.active .group-title {
    color: #FFF;
    background-color: #FF6008;
}
