}
.btn-step {
    min-width: 150px;
    font-size: 1.25em;
    font-weight: bold;
    color: #FF6008;
    background-color: #ffffff;
    border-color: #FF6008;
    border-radius: 40px;
    padding: 4px 10px;
}
.btn-step:hover, .btn-step:focus {
    color: #ffffff;
    background-color: #FF6008;
}
.btn-step[disabled] {
    color: #A9A9A9;
    border-color: #A9A9A9;
    background-color: #FFF;
}
.btn-next {
    color: #ffffff;
    background-color: #FF6008;
    border-color: #FF6008;
    border-radius: 10px;
    min-width: 100px;
    /* padding: 4px 10px*/
}
.btn-next:hover, .btn-next:focus {
    color: #ffffff;
}
.step-header {
    font-weight: bold;
    padding-bottom: 40px;
    padding-top: 20px;
    text-align: center;
}
.step-info {
    color: #B0B9C0;
}
.step-detail {
    color: #FF6008;
    margin-left: 8px;
}
.step-footer {
    padding-top: 40px;
