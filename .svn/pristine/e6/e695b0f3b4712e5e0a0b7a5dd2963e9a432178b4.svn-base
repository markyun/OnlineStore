    color: #FF6008;
}
.group-box.empty {
    width: 132px;
    height: 132px;
    border: 4px dashed #BBB;
    background-clip: content-box;
    color: #999;
    font-size: 1.2rem;
    line-height: 135px;
    text-align: center;
}
.group-title {
    padding-top: 5px;
    height: 45px;
    line-height: 40px;
    color: #333;
    background-color: #C3C3C3;
    text-align: center;
}
.group-content {
    padding-top: 1px;
    text-align: center;
}
.group-input {
    display: block;
    margin: 0 auto;
    padding: 6px 0;
    border: none;
    outline: none;
    text-align: center;
    color: #333;
    border-bottom: 1px solid #DDD;
    width: 50%;
    height: 50px;
    font-size: 2em;
}
.group-image {
    display: inline-block;
    width: 135px;
    height: 135px;
}
.panel-bundle .active {
    width: 200px;
    height: 200px;
    border-color: #FF6008;
    margin: 40px auto 20px;
}
