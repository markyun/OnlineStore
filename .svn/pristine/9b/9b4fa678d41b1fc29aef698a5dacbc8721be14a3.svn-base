<div class="row mand_choose">Choose all or noneï¼š<%=groupName%></div>
<div class="row option-all">
	<%_.each(allTemplate,function (all){%>
	<div class="col-md-3 col-sm-3">
		<div class="panel panel-default js-all-option"
			data-offer-id="<%= all.offerId %>"
			data-offer-groupid="<%=all.offerGroupId%>"
			data-eff-type="<%=all.effType%>" data-pro-necessary="0"
			data-group-type="A">
			<div class="panel-heading"><a class="head_a"><%=all.offerName%></a></div>
			<div class="panel-body">
				<div class="cabin-info">
					<span class="highlight"><%=all.comments%></span>
				</div>
				<%if(all.offerAttrList){if(all.offerAttrList.attrValueDtoList&&one.offerAttrList.inputType===1){%><%=all.offerAttrList.attrName%><select
					class="form-control cabin-select" name="time">
					<%if(all.offerAttrList.nullable === 'Y'){%>
					<option value=""></option> <%}%>
					<%_.each(all.offerAttrList.attrValueDtoList,function(attr){%>
					<option value="<%=attr.attrId%>,<%=attr.value%>"><%=attr.valueMark%></option>
					<%})%>
				</select> <%}}%>
				<div class="cabin-btn">Choose</div>
			</div>
		</div>
	</div>
	<%})%>
</div>
